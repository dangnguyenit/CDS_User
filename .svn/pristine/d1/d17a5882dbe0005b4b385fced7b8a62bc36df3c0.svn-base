  * @param {string} [sub_type] - Sub type for long or short term
  * @param {integer} [user_owner_id] - Owner user of this page
  * @returns None
  * @deprecated since version 2.0
  * @todo improve this function.
  */
	insert_comment : function (e, element, user_id, obj_id, comment_type, sub_type, user_owner_id) {
		if (e.keyCode == 13 && !e.shiftKey) {	
			rs = false

			comment = element.value.trim()
	  	if (comment == "" ) {
	  		CDS.show_notification("error", "Please Enter Comment")
	  	}
	  	else {
	  		rs = true
	  	}  	
		  
		  if (rs) {
		  	click = comment
		  	console.log(comment)
		  	// Call ajax to add comment
			  $.ajax({
			    type: "POST",
			    // async: false,
			    url: g_actionInstancesTermPath,
			    data: {
			    	data: user_id,
			    	user_owner_id: user_owner_id,
			    	obj_id: obj_id,
			    	comment_type: comment_type,
			    	sub_type: sub_type,
			    	comment: comment,
			    	type: "create_comment",
			      authenticity_token: g_token},
			    dataType: "json",
			    success: function(data) {
			    	element.value = ""
			    	if (comment_type == "short_term_objective_id") {
			    		flag_about = false
			    		UserPA.render_comments(this, user_owner_id ,obj_id, 'short_term_objective', 'true');
			    	}
			    	if (comment_type == "current_title_id") {
			    		flag_about = false
			    		UserPA.render_about(user_owner_id, "", "create_" + sub_type );
			    	}
			    	if (comment_type == "evidence_id") {
			    		UserPA.render_comments(this, user_owner_id, obj_id, "evidence", 'true' );
			    	}   	
			    },
			    error: function(data) {
			    	string = data.responseJSON
		        result = ""
		        for(key in string) {
		          for(value in string[key]) {
		            result += CDS.capitaliseFirstLetter(key) + " : " + CDS.capitaliseFirstLetter(string[key][0]) +"<br> " 
		          }
		        }
		        CDS.show_notification("error", result)
			    }
			  });
		  }
		}
	},

