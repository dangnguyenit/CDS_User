.relativeWrap
  .box-generic
    / Tabs Heading
    .tabsbar.tabsbar-2
      %ul.row.row-merge
        %li#current_status_tab.active
          %a.center{"data-toggle" => "tab", href: "#tab1-5"}
            %span.strong Current Status
        %li#recently_added_tab
          %a.center{"data-toggle" => "tab", href: "#tab2-5"}
            %span.strong Recently Added
        %li#all_slot_tab
          %a.center{"data-toggle" => "tab", href: "#tab3-5"}
            %span.strong All Slot
            = @slots.count
        %li#all_other_tab
          %a.center{"data-toggle" => "tab", href: "#tab4-5"}
            %span.strong All Other Subject
            = @other_subjects.count
        %li#short_term_tab
          %a.center{"data-toggle" => "tab", href: "#tab5-5"}
            %span.strong Short-Term Object
    / // Tabs Heading END
    .tab-content
      / Tab content
      #tab1-5.tab-pane.active
        =render partial: 'users/current_status'
      / // Tab content END
      / Tab content
      #tab2-5.tab-pane
        #recently_added
        =#render partial: 'users/recently_added'
      / // Tab content END

